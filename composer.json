{
    "type": "library",
    "name": "kherge/elf",
    "description": "A collection of helpers for the Symfony Console component.",
    "homepage": "http://github.com/kherge/Elf",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "me@kevingh.com"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "justinrainbow/json-schema": "1.1.*",
        "seld/jsonlint": "1.*",
        "symfony/console": "~2.1",
        "symfony/process": "~2.1"
    },

    "autoload": {
        "psr-0": {
            "KevinGH\\Elf": "src/lib/"
        }
    },

    "config": {
        "vendor-dir": "src/vendors"
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
